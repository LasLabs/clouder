<odoo>
    
    <record id="application_type_option_elasticsearch_network"
            model="clouder.application.type.option"
            >
        <field name="application_type_id"
               ref="application_type_elasticsearch"
               />
        <field name="name">network</field>
        <field name="type">service</field>
        <field name="default">172.17.0.0/16</field>
    </record>
    
    <record id="application_type_option_elasticsearch_listen"
            model="clouder.application.type.option"
            >
        <field name="application_type_id"
               ref="application_type_elasticsearch"
               />
        <field name="name">listen</field>
        <field name="type">service</field>
        <field name="default">*</field>
    </record>
    
</odoo>
